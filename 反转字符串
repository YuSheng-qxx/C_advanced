class Solution {
public:
    /**
     * 反转字符串
     * @param str string字符串 
     * @return string字符串
     */
    string solve(string str) {
        // write code here
        int begin=0;
        int end=str.size()-1;
        while(begin<end)
        {
            swap(str[begin++],str[end--]);
        }
        return str;
    }
};
