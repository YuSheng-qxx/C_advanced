#include <stdio.h>

int main()
{
    int i,temp=0,temp1=0,temp2=0;
    int sum=0;
    for(i=1;i<2020;i++)
    {
        temp=(i/100)%10;
        temp1=i/10%10;
        temp2=i%10;
        if((temp==9)||(temp1==9)||(temp2==9))
        {
            sum+=1;
        }
    }
    printf("%d",sum);
    return 0;
}
